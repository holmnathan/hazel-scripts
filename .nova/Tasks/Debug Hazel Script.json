{
  "actions": {
    "clean": {
      "enabled": true,
      "script": "set -euo pipefail\n\nrm -R tmp"
    },
    "run": {
      "enabled": true,
      "script": "set -euo pipefail\n\ncat \\\n  \"src\/library\/environment-utilities.jxa\" \\\n  \"src\/actions\/set-file-creation-date.jxa\" \\\n  \"src\/debug\/run-set-file-creation-date.jxa\" | \\\nosascript -l JavaScript - \"$1\" \"$2\" \"$3\""
    }
  },
  "arguments": ["${WorkspaceName}", "${File}", "${WorkspaceFolder}"],
  "environment": {
    "DEBUG_CREATION_DATE": "{\"year\": 2025, \"month\": 12, \"day\": 25 }",
    "DEBUG_CREATION_TIME": "{\"hour\": 6, \"minute\": 45, \"second\": 5, \"millisecond\": 1}",
    "DEBUG_ENABLED": "true",
    "DEBUG_RESET_TIME": ""
  },
  "openLogOnRun": "start"
}
